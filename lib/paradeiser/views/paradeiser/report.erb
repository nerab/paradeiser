ID | Name | Status | Started | Ended | Interrupts
<% @pom.each do |pom| %>
<% interrupts_by_type = pom.interrupts.group_by{|i| i.type } %>
<%= pom.id %> | <%= pom.name %> | <%= pom.status %> | <%= pom.started_at.strftime('%R') %> | <%= pom.finished_at.strftime('%R') if pom.finished? %> | <%= interrupts[:internal].size rescue 0 %> I, <%= interrupts_by_type[:external].size rescue 0 %> E
<% end %>
